/* 用户注册 */
+ (void)resgisterWithLoginName:(NSString *)loginName
                      passWord:(NSString *)passWord
                          type:(NSString *)type
                       captcha:(NSString *)captcha
                          step:(NSString *)step
                      inviteNo:(NSString *)inviteNo
                      realName:(NSString *)realName
              recommendedPhone:(NSString *)recommendedPhone
                       success:(void (^)(NetworkModel *model))success
                          fail:(void (^)(NSError *error))fail {
    
    if ([[NSString stringWithFormat:@"%@",[KimiUserInfoModel currentUserinfo].pushToken] isEqualToString:@""] ||
        [[NSString stringWithFormat:@"%@",[KimiUserInfoModel currentUserinfo].pushToken] isEqualToString:@"(null)"]) {
        
        NSString *pushToken = [self getDeviceCode];
        [KimiUserInfoModel savePushToken:pushToken sussess:^(NetworkModel *model) {
            if ([model.result isEqualToString:kNetwork_ResultCode]) {
                
                NSMutableDictionary *paras = [[NSMutableDictionary alloc]init];
                [paras setValue:[NSString stringWithFormat:@"%@",loginName] forKey:@"loginName"];
                [paras setValue:[NSString stringWithFormat:@"%@",passWord] forKey:@"passWord"];
                [paras setValue:[NSString stringWithFormat:@"%@",type] forKey:@"type"];
                [paras setValue:[NSString stringWithFormat:@"%@",captcha] forKey:@"captcha"];
                [paras setValue:[NSString stringWithFormat:@"%@",step] forKey:@"step"];
                [paras setValue:[NSString stringWithFormat:@"%@",inviteNo] forKey:@"inviteNo"];
                [paras setValue:[NSString stringWithFormat:@"%@",realName] forKey:@"realName"];
                [paras setValue:[NSString stringWithFormat:@"%@",recommendedPhone] forKey:@"recommendedPhone"];
                [paras setValue:[NSString stringWithFormat:@"%@",pushToken] forKey:@"pushToken"];
                
                [NetworkBase getRequestWithUrl:kNetwork_Register paras:paras success:^(NetworkModel *model) {
                    if (success) {
                        success(model);
                    }
                } fail:^(NSError *error) {
                    if (fail) {
                        fail(error);
                    }
                }];
            }
            else {
                if (success) {
                    NetworkModel *errorModel = [[NetworkModel alloc]init];
                    [errorModel setResult:@"99999"];
                    [errorModel setMsg:@"访问网络数据异常"];
                    [errorModel setData:@""];
                    success(errorModel);
                }
            }
        } fail:^(NSError *error) {
            if (fail) {
                fail(error);
            }
        }];
    }
    else {
        NSMutableDictionary *paras = [[NSMutableDictionary alloc]init];
        [paras setValue:[NSString stringWithFormat:@"%@",loginName] forKey:@"loginName"];
        [paras setValue:[NSString stringWithFormat:@"%@",passWord] forKey:@"passWord"];
        [paras setValue:[NSString stringWithFormat:@"%@",type] forKey:@"type"];
        [paras setValue:[NSString stringWithFormat:@"%@",captcha] forKey:@"captcha"];
        [paras setValue:[NSString stringWithFormat:@"%@",step] forKey:@"step"];
        [paras setValue:[NSString stringWithFormat:@"%@",inviteNo] forKey:@"inviteNo"];
        [paras setValue:[NSString stringWithFormat:@"%@",realName] forKey:@"realName"];
        [paras setValue:[NSString stringWithFormat:@"%@",recommendedPhone] forKey:@"recommendedPhone"];
        [paras setValue:[NSString stringWithFormat:@"%@",[KimiUserInfoModel currentUserinfo].pushToken] forKey:@"pushToken"];
        
        [NetworkBase getRequestWithUrl:kNetwork_Register paras:paras success:^(NetworkModel *model) {
            if (success) {
                success(model);
            }
        } fail:^(NSError *error) {
            if (fail) {
                fail(error);
            }
        }];
    }
}


